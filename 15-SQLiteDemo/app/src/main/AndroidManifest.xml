<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.telecom.sqlitedemo">

    <uses-permission android:name="com.telecom.contactsproviderdemo.READ_WRITE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactsDatabaseActivity"
            android:label="直接访问SQLite数据库">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ContactsProviderActivity"
                  android:label="访问外部的ContentProvider">

        </activity>
    </application>

</manifest>