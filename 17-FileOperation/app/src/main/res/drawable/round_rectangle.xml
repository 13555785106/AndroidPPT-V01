<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <stroke
        android:width="2dip"
        android:color="@android:color/darker_gray"
        android:dashGap="1dip"
        android:dashWidth="4dip"/>
    <solid android:color="@android:color/white"/>
    <corners
        android:bottomLeftRadius="4dip"
        android:bottomRightRadius="4dip"
        android:topLeftRadius="4dip"
        android:topRightRadius="4dip"/>
    <gradient
        android:angle="45"
        android:centerColor="#07ece4"
        android:endColor="#bcbebd"
        android:startColor="#f4eded"
        android:type="linear"/>
</shape>