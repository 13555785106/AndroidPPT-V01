<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.telecom.touchscreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TouchScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MotionEventActivity"
            android:label="MotionEventActivity">
        </activity>
        <activity
            android:name=".ScaleGestureDetectorActivity"
            android:label="ScaleGestureDetectorActivity">
        </activity>
        <activity
            android:name=".GestureDetectorActivity"
            android:label="GestureDetectorActivity">
        </activity>
        <activity
            android:name=".PathLengthActivity"
            android:label="PathLengthActivity">
        </activity>
        <activity
            android:name=".RotateAngleActivity"
            android:label="RotateAngleActivity">
        </activity>
        <activity
            android:name=".GlideImageActivity"
            android:label="GlideImageActivity">
        </activity>
    </application>

</manifest>